{
  "name": "@rainbowmarket/field-text-input-flydown",
  "version": "1.0.1",
  "description": "Custom Blockly Fields with Flydown and Renderer Components",
  "main": "dist/index.js",
  "module": "src/index.js",
  "unpkg": "dist/index.js",
  "type": "module",
  "scripts": {
    "audit:fix": "blockly-scripts auditFix",
    "build": "blockly-scripts build",
    "build-nolint": "blockly-scripts build --skip-lint",
    "clean": "blockly-scripts clean",
    "lint": "blockly-scripts lint",
    "predeploy": "blockly-scripts predeploy",
    "prepublishOnly": "npm run clean && npm run build-nolint",
    "start": "blockly-scripts start --skip-lint",
    "test": "blockly-scripts test"
  },
  "keywords": [
    "blockly",
    "blockly-plugin",
    "blockly-field-flydown"
  ],
  "author": "Naveen",
  "license": "MIT",
  "homepage": "https://github.com/Rainbowmarket/field_text_input_flydown#readme",
  "bugs": {
    "url": "https://github.com/Rainbowmarket/field_text_input_flydown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rainbowmarket/field_text_input_flydown.git"
  },
  "directories": {
    "dist": "dist",
    "src": "src"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "blockly": "^10.4.3",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@blockly/dev-scripts": "^4.0.7",
    "@blockly/dev-tools": "^7.0.2",
    "chai": "^4.3.7",
    "mocha": "^11.1.0",
    "parcel": "^2.10.0"
  },
  "eslintConfig": {
    "extends": "@blockly/eslint-config"
  },
  "engines": {
    "node": ">=8.17.0"
  },
  "browserslist": [
    "defaults",
    "IE 11",
    "IE_Mob 11"
  ],
  "packageManager": "yarn@1.22.22"
}
